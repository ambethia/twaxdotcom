{
  "name": "Twax",
  "description": "Twitter to Fax Gateway",
  "logo": "",
  "repository": "",
  "keywords": [
  ],
  "scripts": {
    "postdeploy": "bundle exec rake db:migrate"
  },
  "env": {
    "MANDRILL_USERNAME": {
      "description": "Your Mandrill address for sending mail.",
      "required": false
    },
    "MANDRILL_APIKEY": {
      "description": "Your Mandrill API key for sending mail.",
      "required": false
    },
    "OMNIAUTH_PROVIDER_KEY": {
      "description": "Credentials from Twitter.",
      "value": "some_long_key",
      "required": true
    },
    "OMNIAUTH_PROVIDER_SECRET": {
      "description": "Credentials from Twitter.",
      "required": true
    },
    "RAILS_ENV": "production"
  }
}
